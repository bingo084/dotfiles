xrandr --output DP-0 --mode 1920x1080 --rate 144 --output eDP-1-1 --off
~/bin/dwm/autochange-wallpaper.sh &
fcitx5 -d
picom -b
flameshot &
xremap ~/.config/xremap/config.yml --watch=device,config &
clash &
while true; do
   xsetroot -name "$( date +"%F %R" )"
   sleep 1m    # Update time every minute
done &
exec ~/bin/dwm/startdwm.sh
